{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@component/*": ["components/*"],
      "@data/*": ["data/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.js", "**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"]
}
