{
  "name": "hyperui",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "dev": "next dev",
    "lint": "next lint",
    "css": "yarn css:components && yarn css:blogs",
    "css:blogs": "npx tailwindcss -i src/styles/blogs.css -o public/blogs.css -m",
    "css:components": "npx tailwindcss -i src/styles/components.css -o public/components.css -m",
    "watch": "yarn css -w",
    "postbuild": "yarn css && next-sitemap --config sitemap.config.js"
  },
  "dependencies": {
    "next": "^13.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.12",
    "eslint": "8.11.0",
    "eslint-config-next": "^13.3.1",
    "fast-glob": "^3.2.12",
    "gray-matter": "^4.0.3",
    "next-mdx-remote": "^4.0.0",
    "next-sitemap": "^3.1.20",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prismjs": "^1.27.0",
    "react-intersection-observer": "^8.33.1",
    "react-use": "^17.4.0",
    "rehype-external-links": "^2.1.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "remark-slug": "^7.0.1",
    "tailwindcss": "^3.3.1"
  }
}
