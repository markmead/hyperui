{
  "name": "hyperui",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "dev": "next dev",
    "lint": "next lint",
    "css": "npm run css:components && npm run css:blogs",
    "css:blogs": "npx tailwindcss -i src/styles/blogs.css -o public/blogs.css -m",
    "css:components": "npx tailwindcss -i src/styles/components.css -o public/components.css -m",
    "postbuild": "npm run css",
    "format": "prettier '{src,app,public}/**/*.{css,js,jsx,mdx,html}' --write"
  },
  "dependencies": {
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.5.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "fast-glob": "^3.3.1",
    "gray-matter": "^4.0.3",
    "next-mdx-remote": "^4.4.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prismjs": "^1.29.0",
    "react-intersection-observer": "^9.5.2",
    "react-use": "^17.4.0",
    "rehype-external-links": "^3.0.0",
    "remark": "^14.0.3",
    "remark-html": "^15.0.1",
    "remark-slug": "^7.0.1",
    "tailwindcss": "^3.3.3"
  }
}
